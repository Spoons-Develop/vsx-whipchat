{
  "name": "whip-chat",
  "displayName": "AI Whiplash",
  "description": "Makes your interactions more \"aggressive\" by playing a whip sound effect when you send a message in the chat.",
  "version": "1.0.0",
  "publisher": "spoonsdevelop",
  "author": {
    "name": "Spoons Develop"
  },
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Spoons-Develop/vsx-whipchat"
  },
  "bugs": {
    "url": "https://github.com/Spoons-Develop/vsx-whipchat/issues"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "chat",
    "copilot",
    "sound",
    "whip",
    "fun",
    "vibe",
    "entertainment",
    "sound-effects",
    "humor",
    "ai"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "whip-chat.enable",
        "title": "AI Whiplash: Enable"
      },
      {
        "command": "whip-chat.disable",
        "title": "AI Whiplash: Disable"
      },
      {
        "command": "whip-chat.toggle",
        "title": "AI Whiplash: Toggle"
      }
    ],
    "keybindings": [
      {
        "command": "whip-chat.submitWithSound",
        "key": "enter",
        "when": "inChat && chatInputHasText"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.2.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "node-wav-player": "^0.2.0"
  }
}